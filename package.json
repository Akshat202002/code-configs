{
  "name": "zx-rp",
  "version": "1.0.0",
  "scripts": {
    "nghm": "node --max_old_space_size=16000 --openssl-legacy-provider ./node_modules/@angular/cli/bin/ng serve",
    "nghmS": "node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng serve",
    "nghmp": "node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng serve --configuration",
    "zxProd": "node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng build -c=production --aot --build-optimizer",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "npx eslint src",
    "lintErr": "npx eslint src --quiet",
    "lintFix": "npx eslint src --fix",
    "lintOut": "npx eslint src > lint-output.txt",
    "lintFixOut": "npx eslint src --fix > lint-output.txt",
    "lintErrOut": "npx eslint src --quiet > lint-output.txt",
    "compile": "tsc --pretty",
    "e2e": "ng e2e",
    "eslint-fix": "npx eslint src --fix --max-warnings 0",
    "formatTs": "prettier --config .prettierrc --write 'src/**/*.ts' '!src/**/*.d.ts' '!src/**/*.spec.ts'",
    "formatHtml": "prettier --config .prettierrc --write 'src/**/*.html'",
    "formatScss": "prettier --config .prettierrc --write 'src/**/*.scss'",
    "customer": "pbjs -t static-module ./proto/customer.proto -o src/app/protos/customer/customer.js && pbts --no-comments src/app/protos/customer/customer.js -o src/app/protos/customer/customer.d.ts",
    "routeTrace": "pbjs -t static-module ./proto/routeTrace.proto -o src/app/protos/route/routeTrace.js && pbts --no-comments src/app/protos/route/routeTrace.js -o src/app/protos/route/routeTrace.d.ts",
    "routeTrace_new": "pbjs -t static-module ./proto/routeTrace_new.proto -o src/app/protos/route/routeTraceNew.js && pbts --no-comments src/app/protos/route/routeTraceNew.js -o src/app/protos/route/routeTraceNew.d.ts",
    "crowFlyDispatch": "pbjs -t static-module ./proto/crowFly_dispatch.proto -o src/app/protos/crowFlyDispatch/crowFlyDispatch.js && pbts --no-comments src/app/protos/crowFlyDispatch/crowFlyDispatch.js -o src/app/protos/crowFlyDispatch/crowFlyDispatch.d.ts",
    "usrOps": "pbjs -t static-module ./proto/usrOps/usrOps.proto -o src/app/protos/usrOps/usrOps.js && pbts --no-comments src/app/protos/usrOps/usrOps.js -o src/app/protos/usrOps/usrOps.d.ts",
    "algoOperation": "pbjs -t static-module ./proto/algoOperation.proto -o src/app/protos/algoOperation/algoOperation.js && pbts --no-comments src/app/protos/algoOperation/algoOperation.js -o src/app/protos/algoOperation/algoOperation.d.ts",
    "dbAlgoOp": "pbjs -t static-module ./proto/dbOperation.proto -o src/app/protos/dbOperation/dbOperation.js && pbts --no-comments src/app/protos/dbOperation/dbOperation.js -o src/app/protos/dbOperation/dbOperation.d.ts",
    "boundary": "pbjs -t static-module ./proto/boundary.proto -o src/app/protos/boundary/boundary.js && pbts --no-comments src/app/protos/boundary/boundary.js -o src/app/protos/boundary/boundary.d.ts",
    "prepare": "husky",
    "copyright": "node copyright-header.js",
    "copyrightAll": "node copyright-header.js --all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@circlon/angular-tree-component": "^11.0.4",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@grapecity/wijmo.angular2.all": "5.20212.812",
    "@juggle/resize-observer": "^3.2.0",
    "@ngneat/hot-toast": "^5.0.1",
    "@ngneat/overview": "^4.1.0",
    "@turf/convex": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/meta": "^6.5.0",
    "@turf/midpoint": "^6.5.0",
    "@turf/nearest-point-on-line": "^6.5.0",
    "@turf/polygon-to-line": "^6.5.0",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^12.20.1",
    "@zignex/zx-context-menu": "3.1.0-next",
    "@zignex/zx-nav-bar": "3.1.0-next",
    "@zignex/zx-ol-map": "3.1.0-next",
    "@zignex/zx-wj-grid": "3.0.0",
    "angular-resizable-element": "^3.3.5",
    "file-saver": "^2.0.5",
    "glob": "^11.0.0",
    "jspdf": "^2.3.1",
    "keycloak-angular": "13.1.0",
    "keycloak-js": "21.1.1",
    "lodash": "^4.17.21",
    "long": "^4.0.0",
    "moment": "^2.29.1",
    "net": "^1.0.2",
    "ng-packagr": "^15.2.2",
    "ngx-dropzone": "^3.1.0",
    "ngx-spinner": "^15.0.1",
    "ol": "9.2.2",
    "protobufjs": "^6.11.4",
    "rxjs": "^6.6.7",
    "rxjs-compat": "^6.6.7",
    "stompjs": "^2.3.3",
    "svg-url-loader": "^7.1.1",
    "tslib": "^2.6.3",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0",
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^8.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.17.5",
    "@types/long": "^4.0.1",
    "@types/stompjs": "^2.3.9",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "autoprefixer": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^9.1.6",
    "jasmine-core": "^4.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.2",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.2.19",
    "ts-node": "~8.3.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.13.0",
    "webpack": "^5.58.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "src/**/!(*.d|*.spec).ts": [
      "npm run formatTs",
      "npm run lintFix",
      "npm run eslint-fix",
      "npm run copyright"
    ],
    "src/**/*.html": [
      "npm run formatHtml",
      "npm run lintFix"
    ],
    "src/**/*.scss": [
      "npm run formatScss"
    ]
  }
}
